  "author": "Tim Oxley <secoif@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.3.26",
    "tape": "^4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timoxley/serializerr.git"
  },
  "bugs": {
    "url": "https://github.com/timoxley/serializerr/issues"
  },
  "homepage": "https://github.com/timoxley/serializerr",
  "dependencies": {
    "protochain": "^1.0.3"
  }
}
